{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTitleText": "消时令",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }, // Fix: Add missing comma
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/game/game",
      "style": {
        "navigationBarTitleText": "消时令"
      }
    },
    {
      "path": "pages/index/components/SeasonalPage",
      "style": {
        "navigationBarTitleText": "时令页面"
      }
    },
    {
      "path": "pages/index/components/ChallengeSeasonalPage",
      "style": {
        "navigationBarTitleText": "彼此时令页面"
      }
    },
    {
      "path": "pages/index/components/SeasonalChanllenge",
      "style": {
        "navigationBarTitleText": "时令挑战页面"
      }
    },
    {
      "path": "pages/index/components/SeasonalReadPage",
      "style": {
        "navigationBarTitleText": "阅读页面"
      }
    },
    {
      "path": "pages/load/load.vue",
      "style": {
        "navigationBarTitleText": "load"
      }
    },
    {
    	"path" : "pages/load/load",
    	"style" : 
    	{
    		"navigationBarTitleText" : ""
    	}
    }
  ]
}
